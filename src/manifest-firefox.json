{
  "manifest_version": 3,
  "name": "Search by Selection On Cambridge",
  "version": "1.0.1",
  "description": "Quickly search selected text on Cambridge Dictionary with a convenient tooltip.",
  "permissions": [
    "activeTab"
  ],
  "author": "antaresjeet",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "applications": {
    "gecko": {
      "id": "searchbyselectiononcambridge@extension"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}